			.book-3 .bk-cover h2 span:last-child,
			.book-3 .bk-left h2 span:last-child,
			.book-3 .bk-content p {
				font-family: Georgia, Times, "Times New Roman", serif;
			}

			.book-3 .bk-left h2 {
				color: #fff;
				font-size: 14px;
				line-height: 40px;
				text-align: center;
			}

			.book-3 .bk-back img {
				width: 200px;
				display: block;
				margin: 30px auto 0;
			}

			.book-3 .bk-back p {
				color: #fff;
