} */

			.book-2 .bk-left h2 {
				font-size: 20px;
				line-height: 40px;
				padding-right: 10px;
				text-align: right;
			}

			.book-2 .bk-back p {
				color: red;
				font-size: 13px;
				font-family: "Big Caslon", "Book Antiqua", "Palatino Linotype", Georgia, serif;
				padding: 40px;
